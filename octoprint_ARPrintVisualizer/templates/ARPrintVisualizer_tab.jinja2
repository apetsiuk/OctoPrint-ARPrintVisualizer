<img data-bind="attr: {src: 'http://127.0.0.1:27100/stream/' + settingsViewModel.settings.plugins.ARPrintVisualizer.stream()}"></img>

<div class="button-container">
	<button id="detection" class="btn" data-bind="disable: isErrorDetected(), click: toggleDetection, css : { 'btn-success': !isDetecting(), 'btn-danger': isDetecting() }">
		Start Error Detection
	</button>
</div>
<div class="button-container">
	<button id="resume" class="btn btn-primary" data-bind="enable: isErrorDetected(), click: resume">
		Resume without Patch
	</button>
</div>
<div class="button-container">
	<button id="patchResume" class="stop btn btn-primary" data-bind="enable: isErrorDetected(), click: patchResume">
		Insert Patch and Resume
	</button>
</div>
